{{#each allNominees}}

<h2 class="heading-year" index="{{@index}}" data-object="year">{{this.Year}}</h2>

<div class="year-container">
   <div class="movie-container">
    {{#each this.Movies}}
      <h3 class="movie-title">{{this.Name}}</h3>
        {{#each this.Viewers}}
        <div class="user-name">{{this.Name}}
          <input type="checkbox" disabled="true" {{#if this.HasSeen}}checked{{/if}}>
        </div>
      {{/each}}
    {{/each}}
    </div>

</div>

{{/each}}

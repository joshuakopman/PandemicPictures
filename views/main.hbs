<button id="randomBtn" onclick="random({{json allNominees}})">Pick Random Movie!</button>

<div id="chosenMovie"></div>
<div class="accordion">
{{#each allNominees}}
  <div class="accordion-title" index="{{@index}}">{{this.Year}}</div>
   <div class="accordion-content">
    {{#each this.Movies}}
      <h4 index="{{@index}}">{{this.Name}}</h2>
        {{#each this.Viewers}}
        <div >{{this.Name}}
          <input type="checkbox" {{#if this.HasSeen}}checked{{/if}}>
        </div>
        <br/>
      {{/each}}
    {{/each}}
    </div>
{{/each}}
</div>